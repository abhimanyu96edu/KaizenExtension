{
    "name": "Kaizen",
    "version": "1.0.0",
    "manifest_version": 2,
    "content_scripts": [
        {
        "matches":[
            "<all_urls>"
        ],
        "js": ["contentscript.js"]
        }
    ],
    "background":{
        "scripts":["backgroundscript.js"]
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "logo.png"
    }
}